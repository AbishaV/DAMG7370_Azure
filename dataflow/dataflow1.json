{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Seattle_Pet_License_TSV",
						"type": "DatasetReference"
					},
					"name": "readTSVfile"
				},
				{
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {License_Issue_Date     License_Number     Animals_Name     Species     Primary_Breed     Secondary_Breed     ZIP_Code} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> readTSVfile",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source2"
			]
		}
	}
}